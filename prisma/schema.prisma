

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
  directUrl = env("DIRECT_URL")
}


model User {
  id         String   @id @default(uuid())
  name       String?
  email      String   @unique
  password   String?
  provider   String   @default("local")
  googleId   String?  @unique
  avatar     String?
  created_at DateTime @default(now())
}

